<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <!-- Package Sources: Defines where NuGet should search for packages -->
  <packageSources>
    <clear />  <!-- Clears existing sources, if you want to redefine -->
    <add key="nuget.org" value="https://api.nuget.org/v3/index.json" />
    <!-- Add more sources if you have private ones -->
    <!-- <add key="PrivateSource" value="https://my-private-nuget-source" /> -->
  </packageSources>

  <!-- Package Restore: Controls package restore behavior -->
  <packageRestore>
    <add key="enabled" value="True" />
    <add key="automatic" value="True" />
  </packageRestore>

  <!-- Global Packages Folder: Set a custom folder for package downloads (optional) -->
  <!-- <config>
    <add key="globalPackagesFolder" value="C:\Custom\NuGet\Packages" />
  </config> -->

  <!-- Other configurations such as trusted signatures, fallback sources, etc. -->
</configuration>